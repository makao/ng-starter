{
  "extends": "eslint:recommended",
  "env": {
    "es6": true,
    "browser": true,
    "commonjs": true,
    "node": true,
    "protractor": true,
    "jasmine": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
        "modules": true
    }
  },
  "globals": {
    "angular": true,
    "module": true,
    "inject": true
  },
  "rules": {
    "no-console": 1,
    "no-unused-vars" : 1,
    "semi": 1
  }
}
